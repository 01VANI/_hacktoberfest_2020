#include<bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n,m;
	    cin>>n>>m;
	    int a[n],b[n];
	    list<int>l;
	    list<int>l1;
	    for(int i=0;i<n;i++)
	      {
	          cin>>a[i];
	          l.push_back(a[i]);
	      }
	    for(int i=0;i<m;i++)
	    {
	      cin>>b[i];
	       l1.push_back(b[i]);
	    }
	    l.merge(l1);
	    for(auto x: l)
	     cout<<x<<" ";
	     cout<<endl;
	}
	
	return 0;
}
